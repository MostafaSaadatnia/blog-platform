{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/index.server.html",
          "/index.csr.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/*.(png|jpg|jpeg|svg|webp)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "rw-articles",
      "urls": [
        "https://api.realworld.show/api/articles*",
        "https://conduit.productionready.io/api/articles*"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "1d",
        "timeout": "5s"
      }
    },
    {
      "name": "rw-article-comments",
      "urls": [
        "https://api.realworld.show/api/articles/**/comments",
        "https://conduit.productionready.io/api/articles/**/comments"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "12h",
        "timeout": "5s"
      }
    },
    {
      "name": "rw-tags",
      "urls": [
        "https://api.realworld.show/api/tags",
        "https://conduit.productionready.io/api/tags"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 50,
        "maxAge": "7d",
        "timeout": "3s"
      }
    }
  ]
}
